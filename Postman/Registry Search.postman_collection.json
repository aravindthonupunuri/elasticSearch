						{
							"key": "token",
							"value": "{{token}}",
							"type": "string"
						}
					]
				},
				"method": "GET",
				"header": [
					{
						"key": "Content-Type",
						"value": "application/json",
						"type": "text"
					},
					{
						"key": "x-api-key",
						"value": "{{key}}",
						"type": "text"
					}
				],
				"url": {
					"raw": "{{search-host}}?first_name=Father&last_name=last&channel=MOBILE&sub_channel=KIOSK",
					"host": [
						"{{search-host}}"
					],
					"query": [
						{
							"key": "first_name",
							"value": "Father"
						},
						{
							"key": "last_name",
							"value": "last"
						},
						{
							"key": "channel",
							"value": "MOBILE"
						},
						{
							"key": "sub_channel",
							"value": "KIOSK"
						}
					]
				}
			},
			"response": []
		}
	],
	"protocolProfileBehavior": {}
