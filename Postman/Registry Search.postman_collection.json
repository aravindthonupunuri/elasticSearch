{
	"info": {
		"_postman_id": "a5343c15-a1bc-4876-9782-17cc770f103c",
		"name": "Registry Search",
		"schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
	},
	"item": [
		{
			"name": "Get Registry by co/registrants name",
			"request": {
				"auth": {
					"type": "bearer",
